{
	"publisher": "琉璃",
	"repository": "https://github.com/glazec/hexo-post-head-generator",
	"name": "hexo-post-head-generator",
	"displayName": "hexo post head generator",
	"description": "autimatically generate the head of the hexo ",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.initHexoHead"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration":[
			{
				"title": "Init hexo head",
				"properties": {
					"initHexoHead.postsPath":{
						"type":"string",
						"description": "The path of the Hexo _posts folder"
					}
				}
			}
		],
		"commands": [
			{
				"command": "extension.initHexoHead",
				"title": "Init hexo head"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
